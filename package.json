  "description": "Circle social media.",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^8.0.0",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "^0.97.20",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.18.97",
    "@types/nodemailer": "^6.4.15",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ua-parser-js": "^0.7.39",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "pre-commit": "^1.2.2",
    "prisma": "^5.15.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "type-fest": "^4.18.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.3.4",
    "@dotenvx/dotenvx": "^0.44.1",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.4.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^1.0.38",
    "winston": "^3.13.0"
  },
  "prisma": {
    "seed": "dotenvx run --env NODE_ENV=development --convention=nextjs -- tsx prisma/seed/seed.ts"
  },
  "pre-commit": [
    "build",
    "add-build"
  ],
  "scripts": {
    "test": "dotenvx run --env NODE_ENV=test --convention=nextjs -- jest",
    "migrate": "dotenvx run --env NODE_ENV=development --convention=nextjs -- prisma migrate dev",
    "postmigrate": "dotenvx run --env NODE_ENV=development --convention=nextjs -- npx @snaplet/seed sync",
    "dev": "dotenvx run --env NODE_ENV=development --convention=nextjs -- nodemon ./server.ts",
    "start": "dotenvx run --env NODE_ENV=production --convention=nextjs -- node ./dist/server.js",
    "prisma:studio": "dotenvx run --env NODE_ENV=development --convention=nextjs -- npx prisma studio",
    "build": "rimraf dist && tsc",
    "swagger": "node ./docs/swagger.js",
    "add-build": "git add dist"
  },
  "_moduleAliases": {
    "@": "./dist/src"
  },
  "@snaplet/seed": {
    "config": "prisma\\seed\\seed.config.ts"
  }
}
