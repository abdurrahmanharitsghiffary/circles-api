{
  "name": "circle_app",
  "version": "0.0.1",
  "description": "Circle social media.",
  "type": "commonjs",
  "devDependencies": {
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "^0.97.20",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.18.97",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "type-fest": "^4.18.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.44.1",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.13.0"
  },
  "prisma": {
    "seed": "dotenvx run --env NODE_ENV=development --convention=nextjs -- tsx prisma/seed/seed.ts"
  },
  "scripts": {
    "test": "dotenvx run --env NODE_ENV=test --convention=nextjs -- jest",
    "migrate": "dotenvx run --env NODE_ENV=development --convention=nextjs -- prisma migrate dev",
    "postmigrate": "dotenvx run --env NODE_ENV=development --convention=nextjs -- npx @snaplet/seed sync",
    "dev": "dotenvx run --env NODE_ENV=development --convention=nextjs -- nodemon ./server.ts",
    "start": "dotenvx run --env NODE_ENV=development --convention=nextjs -- ts-node ./server.ts",
    "prisma:studio": "dotenvx run --env NODE_ENV=development --convention=nextjs -- npx prisma studio",
    "build": "tsc"
  },
  "_moduleAliases": {
    "@": "./src"
  },
  "@snaplet/seed": {
    "config": "prisma\\seed\\seed.config.ts"
  }
}
